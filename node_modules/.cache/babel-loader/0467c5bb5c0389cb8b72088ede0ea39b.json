BA,EAhBA,EAgBA;AAAA;;AACA,UAAA,UAAA,GAAA,KAAA,MAAA,CAAA,SAAA,CAAA,UAAA,IAAA;AAAA,eAAA,IAAA,CAAA,EAAA,KAAA,MAAA,CAAA,MAAA,CAAA,MAAA,CAAA,EAAA;AAAA,OAAA,CAAA;AACA,UAAA,SAAA,GAAA,KAAA,KAAA,CAAA,KAAA,CAAA,SAAA,CAAA,UAAA,IAAA;AAAA,eAAA,IAAA,CAAA,EAAA,KAAA,EAAA;AAAA,OAAA,CAAA;AACA,WAAA,MAAA,CAAA,UAAA,EAAA,KAAA,CAAA,MAAA,CAAA,SAAA,EAAA,CAAA;AACA,MAAA,YAAA,CAAA,OAAA,CAAA,QAAA,EAAA,IAAA,CAAA,SAAA,CAAA,KAAA,MAAA,CAAA;AACA;AArBA;AAVA,CAAA","sourcesContent":["<template>\r\n  <div class=\" w-50 m-auto\">\r\n    <div v-if=\"liste.length.length !== 0\">\r\n    <h1 class=\"text-center\">{{liste.name}}</h1>\r\n    <el-button @click.native=\"openAdd\" class=\"my-3\" type=\"warning\" plain>\r\n      Ajouter un article\r\n    </el-button>\r\n    <div v-if=\"liste.items.length === 0\" class=\"text-center\">\r\n      <h3><i class=\"el-icon-warning\"></i> Aucun élément dans la liste TODO.</h3>\r\n      </div>\r\n    <div v-else>\r\n    <div class=\"card my-1 m-auto\" v-for=\"(item, index) in liste.items\" :key=\"index\">\r\n      <div class=\"card-body py-0 pr-0\">\r\n        <div class=\"row\">\r\n          <div class=\"col-2 align-center\">\r\n            {{item.quantite}}x\r\n          </div>\r\n          <div class=\"col-8 text-left\">\r\n            <label>{{item.name}}</label>\r\n          </div>\r\n          <div @click=\"removeItem(item.id)\" class=\"col-2 align-center text-center\" style=\"background-color:#F56C6C;cursor:pointer;\">\r\n            <i style=\"font-size:1em; color:white;\" class=\"py-2 el-icon-delete\"></i>\r\n          </div>\r\n        </div>\r\n      </div>\r\n      </div>\r\n    </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n<script>\r\n  export default {\r\n    name: 'DetailListe',\r\n\r\n    data: () => ({\r\n      liste: [],\r\n      listes: []\r\n    }),\r\n    mounted() {\r\n      this.getInfosListe();\r\n    },\r\n    methods: {\r\n      getInfosListe() {\r\n        this.listes = JSON.parse(localStorage.getItem('listes')) || []\r\n        const index = this.listes.findIndex(list => list.id === this.$route.params.id)\r\n        console.log(this.liste)\r\n        this.liste = this.listes[index]\r\n      },\r\n\r\n      openAdd() {\r\n                this.$router.push({\r\n                    name: 'addItem',\r\n                    params: {\r\n                        id: this.liste.id\r\n                    }\r\n                })\r\n            },\r\n      removeItem(id) {\r\n        const indexListe = this.listes.findIndex(list => list.id === this.$route.params.id)\r\n        const indexItem = this.liste.items.findIndex(item => item.id === id)\r\n        this.listes[indexListe].items.splice(indexItem, 1)\r\n        localStorage.setItem('listes', JSON.stringify(this.listes))\r\n      }\r\n    }\r\n  }\r\n</script>"],"sourceRoot":"src/components"}]}